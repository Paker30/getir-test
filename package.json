{
  "name": "getir-test",
  "version": "1.1.0",
  "description": "getir backend end",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "docker build . -t paco28/getir-backend:${npm_package_version} -t paco28/getir-backend:latest",
    "start": "node src/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paker30/getir-test.git"
  },
  "keywords": [
    "getir",
    "backend"
  ],
  "author": "Francisco Moreno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Paker30/getir-test/issues"
  },
  "homepage": "https://github.com/Paker30/getir-test#readme",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "express-joi-validation": "^5.0.1",
    "jest": "^27.5.1",
    "mongodb": "^4.4.1"
  },
  "devDependencies": {
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {}
  }
}
